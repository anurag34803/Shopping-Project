<%- include('header') %>

<div class="pt-32 px-10">
    <div class=" flex flex-wrap justify-center">
        <% for(let product of products) { %>
          <div id="<%=product.id%>" class="card mr-6 my-4 cursor-pointer product-cards" onclick="productInfo(event,'<%=product.id%>')">
            <div class="min-w-40 px-4 bg-white border border-inherit rounded-lg">
              <div class="relative flex justify-center items-center">
                <div class="absolute top-0 left-0 bg-green-500 text-white px-2 py-1 rounded-md">
                  <%=product.discount%>% off
                </div>
                  <img class="max-w-48 min-w-48" src="data:image/jpeg;base64," alt="">
              </div>
    
              <div class="hide-text text-lg mt-2">
                <%= product.productname %>
              </div>
      
    
              <div class="flex justify-between items-center text-lg mb-2">
                
                <div>
                  <div class="green-star star-rating flex">
                    <% for(let i=0;i<product.rating;i++) { %>
                    <!-- Filled stars -->
                    <span class="star">★</span>
                    <% } %>
                    <!-- Non-filled stars -->
                    <% for(let i=0;i< (5 - product.rating);i++) { %>
                       <span class="star-empty">★</span>
                    <% } %>
                  </div>
    
                  <span class="curr-price font-semibold">&#x20B9 <%=product.price%></span>
                  <span class="mrp">&#x20B9 <%=product.mrp%></span>
                </div>
                
                <div id="my-cart" onclick="productInfo(event,'<%=product.id%>','S',1)" class="add-to-cart bg-sky-500/100 text-white rounded-md p-2">
                  <i id="cart-icon" class="fa-solid fa-cart-shopping"></i>
                </div>
             
              </div>
            </div>
          </div>
        <% } %>
    </div>
</div>

<%- include('footer') %>